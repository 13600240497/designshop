{% set static = {}
%}
<!-- embed stylesheet begin -->
    <style type="text/css">
	.wrap_U000181_model1 .gs-aigoods .discount-text p.discountText{
		font-size: {{ data.discountFontSize | default(28) / 75}}rem;
		margin-bottom: 0.03rem;
	}
</style>
<!-- embed stylesheet end -->

<div class="gs-goodsWrap gs-aigoods" data-gid="m_zaful_sync">
	<div name="netError" class="net-error">
	  <img  src="https://geshopimg.logsss.com/uploads/24M3UjECuNkOHzl61R5cWrBLGbA7ZyKI.png" / >
		<p class="error-text">The network is abnormal. Please try againÔºÅ</p>
		<div class="re-request">Re-request</div>
	</div>

	<ul class="clearfix" style=" {% if data.isEditEnv == 1 %} padding: 0 11px; {% else %} padding: 0 0.32rem; {% endif %} {% if data.baseBgC %}background-color:{{data.baseBgC}};{% endif %}">
	</ul>

    {% if data.isBottomTextVis == 1 %}
        <div name="ended" class="loading-end">{{data.bottomCopywriter}}</div>	
    {% endif %}
	
	<div class="ui-recommend-loading"></div>
	<div class="geshop_eurogentec_{{pageInstanceId}}"  style="height:17.786rem; background:url(data:image/gif;base64,) repeat center top; background-size:100% auto;"></div>
</div>

<script type="text/html" id="gsTpl_{{pageInstanceId}}" class="gsTpl_zaful_sync">
	<%#
		var result = d;
		var types = {
			'1': 'HOT',
			'2': 'POPULAR',
			'3': 'NEW'
		};
		for(var i = 0; i < result.length; i++){
			var item = result[i];
			var sku = item.goods_sn;
			if(item.promotions[0]){
				var type = item.promotions[0].type;
			}
			var index = i;
	 %>
	 	<%# if(item.is_on_sale == 1){ %>
			<li class="	<%# if(item.goods_number == 0){ %> soldout	<%# } %>" data-index="{{loop.index0}}" style="{% if data.goodsBRadius %}border-radius: {{ data.goodsBRadius|default(12) / 75 }}rem;{% endif %}">
				<%# if(item.promotions[0]){ %>
					<%# if(type == 1){ %>
						{% if data.isDiscountVis|default(1) == 1 %} 
							<div class="discount" style="
							position: absolute;
							text-align:center;
							background-size: 100% 100%;
							border-radius: 50%;
							{# {% if data.isDiscountLeft | default(1) == 1 %}left: 0px; {% else %} right: 0; {% endif %} #}
							{% if data.discountBgColor %}background-color: {{ data.discountBgColor }};{% endif %}
							{% if data.discountFontColor %}color: {{ data.discountFontColor | default('#ffffff') }};{% endif %}
							{% if data.discountBgColor %}background-image: url({{ data.discountBgColor|default('#999999') }});{% endif %}
							{% if data.discountwidth|default(88) > 0 %} width: {{ data.discountwidth|default(88) / 75}}rem;{% else %}width: 0; {% endif %}
							{% if data.discountHeight|default(88) > 0 %}height: {{ data.discountHeight|default(88) / 75}}rem;line-height:{{ data.labelHeight / 75}}rem;{% else %}height: 0; {% endif %}
							{% if data.discountImg %}background-image: url({{ data.discountImg }});{% endif %}
							{% if data.discountTop %}top: {{ data.discountTop / 75 }}rem;{% endif %}
							{% if data.isDiscountLeft|default(1) == 0 %}
								right: {{ data.discountLeft / 75 }}rem;
							{% else %}
								left: {{ data.discountLeft / 75 }}rem;
							{% endif %}
						">
							<div class="discount-text">
								{% if data.isDiscountStyle == 1 %}
									<p class="discountText" style="	
									{% if data.discountTextTop %}padding-top: {{ data.discountTextTop / 75 }}rem;{% endif %}
									{% if data.discountTextLeft %}padding-left: {{ data.discountTextLeft / 75 }}rem;{% endif %}">
									<% item.promotions[0].value %>%</p>
									<p class="offtext" style="{% if data.discountTextLeft %}padding-left: {{ data.discountTextLeft / 75 }}rem;{% endif %}" >{{pageData.off}}</p>
								{% else %}
									<p class="discountText" style="line-height: 0.8rem;
										{% if data.discountTextTop %}padding-top: {{ data.discountTextTop / 75 }}rem;{% endif %}
										{% if data.discountTextLeft %}padding-left: {{ data.discountTextLeft / 75 }}rem;{% endif %}
									"><% item.promotions[0].value %>%</p>
								{% endif %}
							</div>
							{# <% types[item.promotions[0].type] %> #}
						</div>
						{% endif %}
						
						
					<%# }else if(type == 2){ %>
						<div class="mark" style="
							{% if data.labelwidth %} width: {{ data.labelwidth / 75 | default(120/75)}}rem;{% endif %}
							{% if data.labelHeight %}height: {{ data.labelHeight / 75 | default(48/75)}}rem;line-height:{{ data.labelHeight / 75}}rem;{% endif %}
							{% if data.labelBgColor %}background-color: {{ data.labelBgColor | default('#333') }};{% endif %}
							{% if data.labelBgImg %}background-image: url({{ data.labelBgImg }});{% endif %}
							{% if data.labelFontColor %}color: {{ data.labelFontColor | default('#ffffff') }};{% endif %}
							{% if data.isMarkLeft == 1 %}left: 4px;{% endif %}
                            {% if data.goodsBRadius %}border-radius: {{ data.goodsBRadius|default(12) / 75 }}rem 0 0 0;{% endif %}
                            
							">

							<%# if(types[item.promotions[0].type] == 'HOT'){ %>
								{{pageData.hot}}
							<%# }else if(types[item.promotions[0].type] == 'NEW'){ %>
								{{pageData.new}}
							<%# }else if(types[item.promotions[0].type] == 'POPULAR'){ %>
								{{pageData.popular}}
							<%# } %>

						</div>
					<%# } %>
				<%# } %>
				<div class="component-self-goodWrap" style="{% if data.goodsBgc %}background-color:{{data.goodsBgc}}{% endif %};
					padding:{{pageData.gsInnerPadding}}rem {{pageData.gsInnerPadding}}rem 0;
					{% if data.gsRadius%}border-radius:{{data.gsRadius}}px;{% endif %}">
						<div class="gs-img">
								<%# if(item.goods_number == 0){ %>
										<div class="soout">{{pageData.soldOut}}</div>
								<%# } %>
								<a href="<% item.url_title %>"  class="logsss_event goods-img-wrap"
									data-logsss-event-value="{ 'pm':'mr','p':'gs-{{ pageId }}-{{ pageInstanceId }}','ubcta':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','sku':'<% sku  %>','cporder':'{{ layoutIndex }}','rank':'<% index + 1 %>'},'skuinfo':{'sku':'<% sku  %>','pam':'0','pc':'','k':''} }"
								>

									{% if data.isEditEnv == '1' %}
										<img src="<% item.goods_img %>" style="max-width: 100%">
									{% elseif  data.isEditEnv == '0' %}
										<img class="js-geshopImg-lazyload" data-sku="<% sku %>" src="{{data.default.lazyImg|default(data.default.good)}}" data-original="<% item.goods_img %>" style="max-width: 100%; {% if data.goodsBRadius %}border-radius: {{ data.goodsBRadius|default(12) / 75 }}rem {{ data.goodsBRadius|default(12) / 75 }}rem 0 0;{% endif %}"
											data-logsss-browser-value="{ 'pm':'mr','p':'gs-{{ pageId }}-{{ pageInstanceId }}','bv':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','sku':'<% sku  %>','cporder':'{{ layoutIndex }}','rank':'<% index + 1 %>'} }"
										> 
									{% endif %}

								</a>
								
						</div>
						<div class="gs-desc" style="padding: 0.24rem 0.32rem;">
							<a href="<% item.url_title %>" class="logsss_event goods-title"
								data-logsss-event-value="{ 'pm':'mr','p':'gs-{{ pageId }}-{{ pageInstanceId }}','ubcta':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','sku':'<% sku %>','cporder':'{{ layoutIndex }}','rank':'<% index + 1 %>'},'skuinfo':{'sku':'<% sku %>','pam':'0','pc':'','k':''} }"
							>
								<span class="item-name" style="display: block; {% if data.goodsFtC %}color:{{data.goodsFtC}};{% endif %}{% if pageData.goodsFtS %}font-size:{{pageData.goodsFtS}}rem;{% endif %}"><% item.goods_title %></span>
							</a>

							<p class="item-price text-lt">
									<span class="shop-price" style="{% if data.shopPriceColor %}color:{{data.shopPriceColor}};{% endif %}
										{% if pageData.priceFt %}font-size:{{pageData.priceFt}}rem;{% endif %}">
										<strong class="bizhong"></strong>
										<span class="bz_icon"></span>
										<span class="my_shop_price js-refresh-all" data-sku="<% sku %>" data-orgp="<% item.shop_price %>"
									><% item.shop_price %></span>
										</span>

							</p>
							
						</div>
							
				</div>
			</li>
		<%# } %>
	<%# 		} %>
</script>
