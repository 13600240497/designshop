{% set static = {
	discountFtS:data.discountFtS|default(18)
	}
%}



<div class="gs-goodsWrap" data-gid="zaful_index" style="width:{{data.baseWith|default(1280)}}px;">

	<a href="javascript:;" class="recommend-nav-prev"></a>
	<a href="javascript:;" class="recommend-nav-next"></a>
	<ul class="clearfix" style="padding-top:{{data.gsMarginBottom|default('10')}}px;
			padding-right:{{ data.gsPadding|default(10)}}px;
			padding-left: {{ data.gsPadding|default(10)}}px;
			{# margin-left: -{{ data.gsPadding|default(10)}}px; #}
			{% if data.baseBgC %}background-color:{{data.baseBgC}};{% endif %}
	">

	</ul>



</div>

<script type="text/html" id="gsTpl_zaful_default" class="{{pageInstanceId}}">
	<%#
		var result = d;
		for(var i = 0; i < result.length; i++){

			var item = result[i];
			if(item.imgurl && item.imgurl.indexOf('http') !==0){
				item.imgurl = 'https://gloimg.zafcdn.com/' + item.imgurl;
			}else{
				item.imgurl = item.imgurl
			}
			var sku = item.goodssn || item.goodssn2;
			var index = i;
			var url = (typeof DOMAIN == 'undefined'|| typeof GLOBAL == 'undefined')?'javascript:void(0)': DOMAIN + GLOBAL.utilFun.formatSearch(item.goodstitle) + '-p_' + item.goodsid + '.html';
	 %>

					<li class="{{colClass}} goods-item" style="{% if goodsData.colNum %}width:{{100/goodsData.colNum}}%;{% endif %}" data-test="{{data.gsPadding}}">
							<div class="component-self-goodBox" style="margin-bottom:{{data.gsMarginBottom|default('10')}}px;
							">
								<div class="component-self-goodWrap" style="{% if goodsData.goodsBgc %}background-color:{{goodsData.goodsBgc}}{% endif %};
								padding:{{data.gsInnerPadding|default(10)}}px;
								{% if data.gsRadius%}border-radius:{{data.gsRadius}}px;{% endif %}">
										<div class="gs-img" data-editEnv="{{data.isEditEnv}}">
											<a href="<% url %>" style="height:{{data.default.lazyHeight}}px;" class="logsss_event"
												data-logsss-event-value="{ 'pm':'mr','p':'gs-{{ pageId }}-{{ pageInstanceId }}','ubcta':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','sku':'<% item.goodssn %>','cporder':'{{ layoutIndex }}','rank':'<% index + 1 %>'},'skuinfo':{'sku':'<% item.goodssn %>','pam':'0','pc':'<% item.catid %>','k':'<% item.warecode %>'} }"
											>
												{% if data.isEditEnv == '1' and data.goodsInfo %}
													<img  src="{{item.goods_img|default(data.default.good)}}"style="max-width: 100%;max-height:100%;">
												{% elseif  data.isEditEnv == '0' %}
													<img class="js_gdexp_lazy js-geshopImg-lazyload js_gbexp_lazy" src="{{data.default.lazyImg|default(data.default.good)}}" data-original="<% item.imgurl %>"
														style="max-width: 100%;max-height:100%;{% if goodsData.colNum == 4%}width:100%;{% endif %}"
														data-logsss-browser-value="{ 'pm':'mr','p':'gs-{{ pageId }}-{{ pageInstanceId }}','bv':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','sku':'<% item.goodssn %>','cporder':'{{ layoutIndex }}','rank':'<% index + 1 %>'} }"
													>
												{% else %}
													<img  src="<% item.imgurl %>" style="max-width: 100%;max-height:100%;">
												{% endif %}
												{# 上架状态 #}
												{% if data.goodsLimitActive == '1' and item.activity_number == activity_volume_number %}
													<span class="goodStatus">{{ get_component_trans(lang,'sold_out') }}</span>
												{% endif %}
											</a>
											{% if data.discountActive|default() == '1' and item.discount > 0 %}
											<div class="discont-wrap discont-curtoff google-sprite text-center" style="{% if data.discountBgImg %}background:url({{data.discountBgImg}}) no-repeat 0 0/100% 100%;{% endif %}
											{% if goodsData.discountFtC %}color:{{goodsData.discountFtC}};{% endif %}
											{% if goodsData.discountBgc %} background-color:{{goodsData.discountBgc}} {% endif %};
											width:{{data.discountWidth|default(50)}}px;
											height:{{data.discountHeight|default(50)}}px;
											{% if goodsData.discountOffsetT %}top:{{goodsData.discountOffsetT}}px;{% endif %}
											{% if goodsData.discountOffsetR %}right:{{goodsData.discountOffsetR}}px;{% endif %}
											border-radius:{{goodsData.discountRadius}}px;">
												<div class="gs-curtoff-wrap">
													<div class="curtoff-box" style="top:{{data.discountTop|default('0')}}px;left:{{data.discountLeft|default('0')}}px;">
														<strong style="font-size:{{data.discountFtS|default(12)}}px">{{item.discount}}%</strong>
														{% if data.discountSuffix|default('1') == '1' %}<br/>OFF{% endif %}
													</div>
												</div>
											</div>
											{% endif %}
									</div>
									<div class="gs-goods-info">
										<div class="gs-desc">
												{% if data.goodsActive|default(1) == '1' %}
														<a href="{{item.url_title|default('javascript:;')}}" class="logsss_event"
															data-logsss-event-value="{ 'pm':'mr','p':'gs-{{ pageId }}-{{ pageInstanceId }}','ubcta':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','sku':'<% item.goodssn %>','cporder':'{{ layoutIndex }}','rank':'<% index + 1 %>'},'skuinfo':{'sku':'<% item.goodssn %>','pam':'0','pc':'<% item.catid %>','k':'<% item.warecode %>'} }"
														>
																<span class="item-name" style="{% if goodsData.goodsFtC %}color:{{goodsData.goodsFtC}};{% endif %}{% if data.goodsFtS %}font-size:{{data.goodsFtS}}px;{% endif %}"><% item.goodstitle %></span>
														</a>
												{% endif %}

												<p class="item-price">
														{% if data.priceActive|default(1) == '1' %}
														<span class="shop-price">
															{% if data.priceBeforeActive|default(0) == '1' %}
															<span class="my_shop_price_prefix" style="
															font-size:{{data.priceBeforeFtS|default(14)}}px;
															color:{{data.priceBeforeFtC|default('#e1679d')}};
															">
																	{{data.priceBeforeText|default('EXCLUSIVE:')}}
															</span>
															{% endif %}

															<strong class="bizhong"></strong>
															<span class="my_shop_price js-refresh-all hidden" data-sku="<% item.goodssn %>" data-orgp="{{item.shop_price}}" style="{% if goodsData.priceC %}color:{{goodsData.priceC}};{% endif %}{% if goodsData.priceFt %}font-size:{{goodsData.priceFt}}px;{% endif %}">
															$0.00</span>
														</span>
														<span class="js-price-mark-<% item.goodssn %>"></span>
														{% endif %}

														{% if data.marketActive|default(1) == '1' and item.market_price %}
														<del class="market-price" style="{% if goodsData.marketPriceFt %}font-size:{{goodsData.marketPriceFt}}px;{% endif %}"><span class="my_shop_price" data-orgp="{{item.market_price}}"
														style="
														{% if data.marketPriceC %}color:{{data.marketPriceC}};{% endif %}
														">${{item.market_price}}</span></del>
														{% endif %}
												</p>

												{# {% if data.goodsTimeActive|default(0) == '1' %}
												<span class="promotion-time-inner">
												{% if item.left_time and item.left_time>=0 %}
													<i class="icon-clock-black"></i>
													<span class="gs_countDown" data-left-time="{{item.left_time}}"><strong>00 day(S)</strong>00:00:00</span>
												{% endif %}
												</span>
												{% endif %}

												{% if data.goodsLimitActive|default(0) == '1' %}
													{{include ('@app/files/parts/temp/goods_limit.twig')}}
												{% endif %} #}
										</div>
									</div>

								</div>
							</div>
					</li>

	<%# 		} %>
</script>
