{
    "name": "ego/yii-base",
    "description": "yii2基础框架",
    "autoload": {
        "psr-4": {
            "ego\\": "src/",
            "Protobuf\\": "src/soa/socket/Protobuf",
            "GPBMetadata\\": "src/soa/socket/GPBMetadata"
        }
    },
    "repositories": [
       {
           "type": "composer",
           "url": "http://www.composer-satis.com.master.test50.egomsl.com"
       }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http":false
    },
    "autoload-dev": {
        "psr-4": {
            "ego\\tests\\": "tests/",
            "app\\": "tests/app/"
        }
    },
    "require": {
        "globalegrow/php-profile": "^1.0",
        "globalegrow/yii-predis": "^1.0",
        "globalegrow/rabbitmq": "^1.0",
        "globalegrow/base": "^1.0",
        "globalegrow/php-diff": "^1.0",
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-redis": "^2.0",
        "yiisoft/yii2-debug": "^2.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "yiisoft/yii2-smarty": "^2.0",
        "yiisoft/yii2-swiftmailer": "^2.1",
        "aws/aws-sdk-php": "^3.4",
        "smarty/smarty": "^3.1",
        "phpspec/php-diff": "^1.1",
        "google/protobuf": "^3.3"
    }
}
