{
    "name": "globalegrow/rabbitmq",
    "description": "rabbitmq",
    "keywords": ["rabbitmq"],
    "license": "MIT",
    "authors": [
        {
            "name": "mashanling",
            "email": "mashanling@globalegrow.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://www.composer-satis.com.master.test50.egomsl.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http":false
    },
    "autoload": {
        "psr-4": {
            "Globalegrow\\RabbitMQ\\": "src",
            "xlogger\\": "src/v1.4-beta/xlogger"
        },
        "classmap": ["src/XcmqClient_v2.2-beta.01/XcmqClient.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Globalegrow\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=5.6",
        "globalegrow/base": "^1.0"
    },
    "require-dev": {
        "phpunit/php-token-stream": "<2",
        "phpunit/phpunit": "^5"
    }
}
