{
    "name": "globalegrow/php-profile",
    "description": "php性能分析",
    "keywords": ["php", "profile", "xhprof"],
    "license": "MIT",
    "authors": [
        {
            "name": "mashanling",
            "email": "mashanling@globalegrow.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://www.composer-satis.com.master.test50.egomsl.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http":false
    },
    "autoload": {
        "psr-4": {
            "Globalegrow\\PhpProfile\\": ["src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Globalegrow\\Tests\\": ["tests"]
        }
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "globalegrow/base": "^1.0",
        "phpunit/php-token-stream": "<2",
        "phpunit/phpunit": "^5"
    }
}
