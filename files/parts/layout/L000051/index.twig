
<div class="geshop-layout-box layout-drag geshop-container geshop-gb-component ui-{{ pageInstanceId }}" data-key="L000051" id="{{ pageInstanceId }}" style="
	{% if customData.background_color %}background-color: {{ customData.background_color }};{% endif %}
	{% if customData.background_img %}background-image: url({{ customData.background_img }}); background-position: center top; background-repeat: no-repeat;{% endif %}
">
	<style>
    /*背景图*/
.geshop-gb-component.ui-{{ pageInstanceId }}{
	{% if customData.base_box_bgImage %}
		background-image:url({{ customData.background_img }});
	{% endif %}
	{% if customData.background_color %}
	background-color: {{ customData.background_color }};
	{% endif %}
}
	@media screen and (max-width: 1200px){
		.geshop-gb-component.ui-{{ pageInstanceId }} .geshop-row-1200W{
				width: 100%;
		}
	}
	@media screen and (min-width: 1201px){
		.geshop-gb-component.ui-{{ pageInstanceId }} .geshop-row-1200W{
				width: 1200px;
				margin:0 auto;
		}
	}


</style>

<div class="geshop-row geshop-row-1200W">
		<div class="geshop-col-12" data-position="1">
			<div class="component-drop">{{ data[1] }}</div>
		</div>
	</div>
</div>
