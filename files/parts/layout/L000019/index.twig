<div class="geshop-layout-box layout-drag geshop-container geshop-container-L000019" data-key="L000019" id="{{ pageInstanceId }}" style="
	{% if customData.background_color %}background-color: {{ customData.background_color }};{% endif %}
	{% if customData.background_img %}background-image: url({{ customData.background_img }});{% endif %}
	{% if customData.background_repeat_position %}background-position: {{ customData.background_repeat_position }};{% endif %}
	{% if customData.background_repeat %}background-repeat: {{ customData.background_repeat }};{% endif %};
	background-size: 100% auto;
">
	{% if customData.background_img %}
		<input type="hidden" name="imgUrl"  value="{{customData.background_img}}" />
	{% endif %}
	<input type="hidden" name="isEditEnv" value="{{data.isEditEnv}}" />
	<div class="geshop-row">
		<div class="geshop-col-12" data-position="1">
			<div class="component-drop">{{ data[1] }}</div>
		</div>
  </div>
</div>

