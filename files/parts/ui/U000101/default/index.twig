<section class="gb_wrapper_common" style="width:1000px;margin:0 auto;padding: 50px 90px; {% if data.goodsInfo|length == 0 and data.isEditEnv == 0 %}display:none;{% endif %}">
<div class="gb_goodsWrap">
	<li class="gs-subjectGoodItem gs-list-item" data-track-key="{{pageData.rightGoodsInfo.goods_sn}}">
	<div class="gb_leftGood">
		{% if pageData.recommended_active == "1" %}<div class="gb_recommended" style="{% if data.recommended_bgImage %}background:url({{data.recommended_bgImage}}) no-repeat 0 0/100% 100%;{% endif %}"><div><span>{{pageData.discount_text}}</span></div></div>{% endif %}
		
		{# goodDiscount公共模块需要item字段，item字段为goodsInfo里的一项 #}
		{% set item = pageData.rightGoodsInfo %}

		{# 折扣标配置 #}
		{{include ('@app/files/parts/temp/gb/common/default_pc/goodDiscount.twig')}}
		

		<div class="gb_imgBox gs-good-label-box">
		{#  #}
			<a href="{{pageData.rightGoodsInfo.url_title}}"class="track-goodsLink">
				{% if  pageData.rightGoodsInfo.goods_img  %}
					<img  class="js-lazyload" data-lazy="{{pageData.rightGoodsInfo.goods_img}}"  src="{{pageData.rightGoodsInfo.goods_img}}" title="{{pageData.rightGoodsInfo.goods_title}}" alt="{{pageData.rightGoodsInfo.goods_title}}">
				{% else %}
					<img class="js-lazyload" data-lazy="{{pageData.imgSrc}}" src="{{pageData.imgSrc}}">
				{% endif %}

				<i class="gs-goods-status" style="{% if  pageData.rightGoodsInfo.startTime == 0 and pageData.rightGoodsInfo.stock_num <= 0 and pageData.rightGoodsInfo != {}  %} display:block; {% endif %}"><span>{{gb_component_trans(lang , 'sold_out')}}</span></i>
			</a>
			{# 服务标 #}
			{% set labelLeft = 0 %}
			
			{{include ('@app/files/parts/temp/gb/common/default_pc/goodLabel.twig')}}
		</div>
	</div>
	<div class="gb_rightGood">
		<a href="{{pageData.rightGoodsInfo.url_title}}" class="track-goodsLink"><p class="gb_goodsName">{{pageData.rightGoodsInfo.goods_title|default('This is a commodity')}}</p></a>
			{% if pageData.preferential_active == "1" %}
				{% if  pageData.rightGoodsInfo == {} or pageData.rightGoodsInfo.startTime > 0  %}
					<p class="gb_times">
						<em class="icon-clock"></em><span class="gs-goodsRushDown gb_endTime" data-begin="{{pageData.rightGoodsInfo.startTime}}" data-end="{{pageData.rightGoodsInfo.endTime}}" data-stock="{{pageData.rightGoodsInfo.stock_num}}">Ends in: 00:00:00:00</span>
					</p>
				{% endif %}
			{% endif %}

			{% if  pageData.rightGoodsInfo.startTime > 0  %}
				<span class="gs-goodsRushDown" style="opacity:0;display:block;height:0;width:0;overflow:hidden;" data-begin="{{pageData.rightGoodsInfo.startTime}}" data-end="{{pageData.rightGoodsInfo.endTime}}" data-stock="{{pageData.rightGoodsInfo.stock_num}}"></span>
			{% endif %}
		<div class="gb_goodBorder"></div>
		<div class="gb_contentBox">
			{% if pageData.rightGoodsInfo == {}  or pageData.right_contents1 != "" %} <p class="gb_contents">{{pageData.right_contents1|default('This is a product description 1')}}</p>{% endif %}
			{% if pageData.rightGoodsInfo == {}  or pageData.right_contents2 != "" %}<p class="gb_contents">{{pageData.right_contents2|default('This is a product description 2')}}</p>{% endif %}
			{% if pageData.rightGoodsInfo == {}  or pageData.right_contents3 != "" %}<p class="gb_contents">{{pageData.right_contents3|default('This is a product description 3')}}</p>{% endif %}
		</div>

		<p class="gb_price2 gs-shop-price">
			<span class="js-currency gs-sale-price" data-currency="{{pageData.rightGoodsInfo.shop_price|default('0.00')}}">${{pageData.rightGoodsInfo.shop_price|default('0.00')}} </span>
			{% if pageData.rightGoodsInfo.discount > 0 %}
				{% if pageData.price_label_active == '1' %}
				    {% if pageData.rightGoodsInfo.market_price|default('0.00') and pageData.rightGoodsInfo.shop_price|default('0.00') != pageData.rightGoodsInfo.market_price|default('0.00') and pageData.rightGoodsInfo.shop_price|default('0.00') < pageData.rightGoodsInfo.market_price|default('0.00') %}
						<span class="gb_price1"> <strike class="js-currency" data-currency="{{pageData.rightGoodsInfo.market_price|default('0.00')}}">${{pageData.rightGoodsInfo.market_price|default('0.00')}}</strike> </span>
					{% endif %}
				{% endif %}
			{% endif %}

			{% if pageData.rightGoodsInfo.leftQty != "0" and pageData.limt_num_active == '1' %}
				<span class="gb_limitTime"> {{gb_component_trans(lang , 'limited_to_units' , ['<span>'~ pageData.rightGoodsInfo.leftQty|default(0) ~'</span>'])}}</span>  
			{% endif %}
		</p>

		<div class="gb_footer">
			{% if pageData.rightGoodsInfo == {} or data.right_code %}
				<span  class="gb_coupon">{{gb_component_trans(lang , 'coupon')}}: <span class="gb_right_coupon">{{data.right_code|default('ASDF58490')}}</span> </span><span class="gb_copy" data-code="{{data.right_code}}">{{gb_component_trans(lang , 'copy')}}</span>
			{% endif %}

			<a  data-lang="{{lang}}"  class="track-goodsLink gb_buy buyLink {% if pageData.rightGoodsInfo.stock_num <= 0 and pageData.rightGoodsInfo != {} %} cart_good_soldOut {% endif %}"  href="{{pageData.rightGoodsInfo.url_title}}">
				{% if pageData.rightGoodsInfo.stock_num <= 0 and  pageData.rightGoodsInfo != {} %}
					{{gb_component_trans(lang , 'sold_out')}}
				{% else %}
					{{ pageData.shop_text|default(gb_component_trans(lang , 'buy_now'))}}
				{% endif %}
			</a>

			<i class="icon-cart gb_cart  js-addCart   {% if  pageData.rightGoodsInfo.stock_num <= 0 and  pageData.rightGoodsInfo != {} %}  {% if pageData.rightGoodsInfo.startTime == '0' %} cart_good_soldOut {% endif %} {% endif %}"   data-sku="{{pageData.rightGoodsInfo.goods_sku}}" data-warehousecode="{{pageData.rightGoodsInfo.wh_code}}" data-img="{{pageData.rightGoodsInfo.goods_img}}" data-qty="1"></i>
			</a>
		</div>
	</div>

	<li>
</div>
</section>
</div>


