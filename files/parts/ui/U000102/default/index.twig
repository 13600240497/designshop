{% set liCol = ['geshop-col-12','geshop-col-6','geshop-col-4','geshop-col-3','geshop-col-2','geshop-col-1']%}
{% set suffix ='rem'%}
{% set remUnit = 75 %}

{% if data.colNum == '1'%}
    {% set colClass = liCol[0] %}
{% elseif data.colNum == '2' %}
    {% set colClass = liCol[1] %}
{% elseif data.colNum == '3' %}
    {% set colClass = liCol[2] %}
{% elseif data.colNum == '4' %}
    {% set colClass = liCol[3] %}
{% elseif data.colNum == '5' %}
    {% set colClass = liCol[4] %}
{% else %}
    {% set colClass = liCol[3] %}
{% endif %}





{%
    set pageData = {
			price_ft_weight:data.price_ft_weight|default(1),
			base_bg_shadow:data.base_bg_shadow|default(0),
			lang:data.lang|default('en'),
        	imgSrc:'https://geshopcss.logsss.com/imagecache/geshop-test/resources/images/default/good.png',

			base_magin_bottom:(data.base_magin_bottom|default(32)/remUnit)|number_format(3, '.'),
			base_magin_top:(data.base_magin_top|default(32)/remUnit)|number_format(3, '.'),
			base_bgc:data.base_bgc is not null ? data.base_bgc:'#FFFFFF',
			base_bgImage:data.base_bgImage,
			right_sku:data.right_sku is not null ? data.right_sku:'',
			right_code:data.right_code is not null ? data.right_code:'',
			right_contents1:data.right_contents1 is not null ? data.right_contents1:'',
			right_contents2:data.right_contents2 is not null ? data.right_contents2:'',
			right_contents3:data.right_contents3 is not null ? data.right_contents3:'',
			right_bgc:data.right_bgc is not null ? data.right_bgc:'#FFFFFF',
			goods_fts:(data.goods_fts|default(24)/remUnit)|number_format(3, '.')~suffix,
			goods_color:data.goods_color is not null ? data.goods_color:'#333333',
			goods_num_color:data.goods_num_color is not null ? data.goods_num_color:'#CB0403',
			goods_num_bgc:data.goods_num_bgc is not null ? data.goods_num_bgc:'#FFEEEE',
			goods_code_color:data.goods_code_color is not null ? data.goods_code_color:'#FF8A00',
			goods_code_bgc:data.goods_code_bgc is not null ? data.goods_code_bgc:'#FF8A00',
			good_times_color:data.good_times_color is not null ? data.good_times_color:'#CC0000',
			good_introduce:data.good_introduce is not null ? data.good_introduce:'#999999',
			shop_btn_bgc:data.shop_btn_bgc is not null ? data.shop_btn_bgc:'#CC0000',
			shop_btn_hover_bgc:data.shop_btn_hover_bgc is not null ? data.shop_btn_hover_bgc:'#FFAE5A',
			shop_btn_color:data.shop_btn_color is not null ? data.shop_btn_color:'#FFFFFF',
			shop_btn_hover_color:data.shop_btn_hover_color is not null ? data.shop_btn_hover_color:'#FFFFFF',
			shop_car_bgc:data.shop_car_bgc is not null ? data.shop_car_bgc:'#FF8A00',
			shop_text:data.shop_text,
			shop_text_size:(data.shop_text_size|default(24)/remUnit)|number_format(3, '.')~suffix,
			dynamic_font_color:data.dynamic_font_color is not null ? data.dynamic_font_color:'#f30240',
			dynamic_font_size:(data.dynamic_font_size|default(22)/remUnit)|number_format(3, '.')~suffix,
			times_font_color:data.times_font_color is not null ? data.times_font_color:'#999999',
			shop_price_color:data.shop_price_color is not null ? data.shop_price_color:'#FF8A00',
			down_time_fts:(data.down_time_fts|default(24)/remUnit)|number_format(3, '.')~suffix,
			down_time_color:data.down_time_color is not null ? data.down_time_color:'#FFFFFF',
			down_time_bgc:data.down_time_bgc is not null ? data.down_time_bgc:'#333333',
			discount_text:data.discount_text is not null ? data.discount_text:'text',
			discount_active:data.discount_active|default(1),
			discount_height:(data.discount_height|default(80)/remUnit)|number_format(3, '.'),
			discount_width:(data.discount_width|default(80)/remUnit)|number_format(3, '.'),
			discount_fts:(data.discount_fts|default(24)/remUnit)|number_format(3, '.')~suffix,
			discount_off:(data.discount_off|default(18)/remUnit)|number_format(3, '.')~suffix,
			discount_marginR:(data.discount_marginR|default(0)/remUnit)|number_format(3, '.')~suffix,
			discount_marginT:(data.discount_marginT|default(0)/remUnit)|number_format(3, '.')~suffix,
			discount_bgImage:data.discount_bgImage,
			discount_bgc:data.discount_bgc is not null ? data.discount_bgc:'#FF8A00',
			discount_ftc:data.discount_ftc is not null ? data.discount_ftc:'#FFFFFF',
			recommended_active:data.recommended_active|default(0),
			recommended_text:data.recommended_text is not null ? data.recommended_text:'text',
			recommended_fts:(data.recommended_fts|default(24)/remUnit)|number_format(3, '.')~suffix,
			recommended_marginT:(data.recommended_marginT|default(0)/remUnit)|number_format(3, '.')~suffix,
			recommended_marginR:(data.recommended_marginR|default(0)/remUnit)|number_format(3, '.')~suffix,
			recommended_bgc:data.recommended_bgc is not null ? data.recommended_bgc:'#FEC455',
			recommended_ftc:data.recommended_ftc is not null ? data.recommended_ftc:'#333333',
			recommended_width:(data.recommended_width|default(68)/remUnit)|number_format(3, '.'),
			recommended_height:(data.recommended_height|default(68)/remUnit)|number_format(3, '.'),
			recommended_bgImage:data.recommended_bgImage,
			preferential_active:data.preferential_active|default(1),
			preferential_num_fts:(data.preferential_num_fts|default(14)/remUnit)|number_format(3, '.')~suffix,
			preferential_code_fts:(data.preferential_code_fts|default(28)/remUnit)|number_format(3, '.')~suffix,
			preferential_codeborder_active:data.preferential_codeborder_active|default(0),
			preferential_copy_ftc:data.preferential_copy_ftc is not null ? data.preferential_copy_ftc:'#FFFFFF',
			preferential_copy_bgc:data.preferential_copy_bgc is not null ? data.preferential_copy_bgc:'#FF8A00',
			rightGoodsInfo : data.goodsInfo[data.right_sku]|default({}),

			server_label_active: data.server_label_active|default(0),
			price_label_active: data.price_label_active|default(1),
			limt_num_active: data.limt_num_active|default(1)
    }
%}

{# {% set goodsStatus =  pageData.rightGoodsInfo.stock_num == 0 and pageData.rightGoodsInfo != {} ? 'good_soldOut' :'goodsBuyPro'%} #}


<div class="geshop-component-box component-drag geshop-gb-component ui-{{ pageInstanceId }}" data-gid="U000102_default" data-key="U000102"  data-buytext="{{pageData.shop_text|default(gb_component_trans(lang , 'buy_now'))}}" data-id="{{ pageInstanceId }}"
	style="{% if pageData.base_magin_bottom %}margin-bottom: {{ pageData.base_magin_bottom }}{{suffix}};{% endif %}margin-top:{{pageData.base_magin_top}}{{suffix}}"
>
<style type="text/css">

    [data-id="{{ pageInstanceId }}"] {
		overflow: initial!important;
	}

	[data-id="{{ pageInstanceId }}"] .gb_goodsWrap{
		position:relative;
		width:{{(750/remUnit)|number_format(3, '.')~suffix}};
		min-height:{{(340/remUnit)|number_format(3, '.')~suffix}};
		overflow: initial!important;
	}

	[data-gid="U000102_default"][data-id="{{ pageInstanceId }}"] .gb_goods_item {
		padding: 0.266rem;
	}

	{# height:{{(550/remUnit)|number_format(3, '.')~suffix}}; #}
	[data-id="{{ pageInstanceId }}"] .gb_main{
		padding: {{(10/remUnit)|number_format(3, '.')~suffix}};
		min-height:{{(167/remUnit)|number_format(3, '.')~suffix}};
	}
	{# height:{{(360/remUnit)|number_format(3, '.')~suffix}};  #}


	[data-id="{{ pageInstanceId }}"] .gb_stylenone{
		background-color:{{pageData.base_bgc}};
	}
	[data-id="{{ pageInstanceId }}"] .gb_imgBox{
		position:relative;
	}

	[data-id="{{ pageInstanceId }}"] .gb_coupon_box{
		height:{{(48/remUnit)|number_format(3, '.')~suffix}}
	}
	[data-id="{{ pageInstanceId }}"] .gb_coupon{
		float:left;
		display:inline-block;
		{% if pageData.preferential_codeborder_active != "1" %}border:{{(2/remUnit)|number_format(3, '.')~suffix}}  dashed  {{pageData.goods_code_bgc}} {% endif %} ;
		{% if pageData.preferential_codeborder_active == "1" %}border:{{(2/remUnit)|number_format(3, '.')~suffix}}  solid  {{pageData.goods_code_bgc}} {% endif %} ;
		border-right:0;
		color:{{pageData.goods_code_color}};
		height:{{(50/remUnit)|number_format(3, '.')~suffix}};
		line-height:{{(50/remUnit)|number_format(3, '.')~suffix}};
		text-align:center;
		font-size:{{pageData.preferential_code_fts}};
		width:{{(280/remUnit)|number_format(3, '.')~suffix}};
		overflow: hidden;
		white-space: nowrap;
	    text-overflow: ellipsis;
	}
	[data-id="{{ pageInstanceId }}"] .gb_copy{
		cursor:pointer;
		float:left;
		max-width: {{(100/remUnit)|number_format(3, '.')~suffix}};
		display:inline-block;
		overflow: hidden;
		text-overflow: ellipsis;
		white-space: nowrap;
		height:{{(50/remUnit)|number_format(3, '.')~suffix}};
		line-height:{{(50/remUnit)|number_format(3, '.')~suffix}};
		color:{{pageData.preferential_copy_ftc}};
		background:{{pageData.preferential_copy_bgc}};
		border-top: {{ (2/remUnit)|number_format(3, '.')~suffix}} solid  {{pageData.preferential_copy_bgc}};
		border-bottom: {{(2/remUnit)|number_format(3, '.')~suffix}} solid {{pageData.preferential_copy_bgc}};
		font-size:{{pageData.preferential_code_fts}};
	}


	[data-id="{{ pageInstanceId }}"] .gb_recommended{
		position:absolute;
		top:{{pageData.recommended_marginT}};
		left:{{pageData.recommended_marginR}};
		width:{{pageData.recommended_width}}rem;
		height:{{pageData.recommended_height}}rem;
		{% if pageData.recommended_bgImage == "" %}
			border-bottom-left-radius: {{pageData.recommended_height /2}}rem;
    		border-bottom-right-radius: {{pageData.recommended_height /2}}rem;
			background:{{pageData.recommended_bgc}};
		{% endif %}
		color:{{pageData.recommended_ftc}};
		font-size:{{pageData.recommended_fts}};
		text-align:center;
	}

	[data-id="{{ pageInstanceId }}"] .gb_recommended div{
		width:{{pageData.recommended_width}}rem;
		height:{{pageData.recommended_height}}rem;
		display:table;
	}

	[data-id="{{ pageInstanceId }}"] .gb_recommended div span{
		word-break: break-word;
		white-space: pre-wrap;
		word-wrap: break-word;
		display:table-cell;
		vertical-align:middle
	}
	[data-id="{{ pageInstanceId }}"]  .gb_discount{
		position:absolute;
		text-align:center;
		display:table;
		line-height:1;
		border-radius: 50%;
	}

	[data-id="{{ pageInstanceId }}"] .gb_discount div{
		font-size:{{pageData.discount_fts}};
		display:table-cell;
		vertical-align:middle;
	}
	[data-id="{{ pageInstanceId }}"] .gb_discount strong{
		display:block;
		font-weight: normal;

	}
	[data-id="{{ pageInstanceId }}"] .gb_discount span{
		font-size:{{pageData.discount_off}};
	}

	[data-id="{{ pageInstanceId }}"] .gb_after_coupon{
		color:{{pageData.dynamic_font_color}};
		height:  {{ (24/remUnit)|number_format(3, '.')~suffix }};
		line-height: {{ (24/remUnit)|number_format(3, '.')~suffix }};
	}
	[data-id="{{ pageInstanceId }}"] .gb_price1{
		color: {{pageData.times_font_color}};
	}
	[data-id="{{ pageInstanceId }}"] .gb_price1 strike {
		font-size: {{pageData.dynamic_font_size}};
		font-weight: normal !important;
	}
	{# [data-id="{{ pageInstanceId }}"] .gb_price2{
		color:{{pageData.dynamic_font_color}};
		font-size: {{pageData.dynamic_font_size}} !important;
	} #}

	[data-id="{{ pageInstanceId }}"] .gb_buy{
		cursor:pointer;
		display:inline-block;
		color:{{pageData.shop_btn_color}};
		text-align:center;
		background:{{pageData.shop_btn_bgc}};
		font-size:{{pageData.shop_text_size}};
		white-space:nowrap;
	}


	[data-id="{{ pageInstanceId }}"] .gb_cart{
		background:{{pageData.shop_car_bgc}};
		color:#FFFFFF;
		text-align:center;
		cursor:pointer;
	}


	[data-id="{{ pageInstanceId }}"] .gb_goodsName{
		color:{{pageData.goods_color}};
		overflow: hidden;
		white-space: nowrap;
	    text-overflow: ellipsis;
	}

	[data-id="{{ pageInstanceId }}"] .gb_endTime{
	}

	[data-id="{{ pageInstanceId }}"] .gb_times{
		height:{{(48/remUnit)|number_format(3, '.')~suffix}};
		line-height:{{(48/remUnit)|number_format(3, '.')~suffix}};
		color:{{pageData.down_time_color}};
		font-size:{{pageData.down_time_fts}};
		background-color:{{pageData.down_time_bgc}};
		text-align:center;
	}

	[data-id="{{ pageInstanceId }}"] .gb_times em{
		font-size:{{pageData.down_time_fts}};
	}

	[data-id="{{ pageInstanceId }}"] .gb_contentBox{
		overflow:hidden;
	}
	{# height:{{(130/remUnit)|number_format(3, '.')~suffix}}; #}
	[data-id="{{ pageInstanceId }}"] .gb_contents{
		font-size:{{pageData.goods_fts}};
		color:{{pageData.good_introduce}};
	}

    /*背景图*/
	{# .geshop-gb-component.ui-{{ pageInstanceId }} .gb_wrapper_common{
	{% if data.base_box_bgImage %}
		background-image:url({{ data.base_box_bgImage }});
	{% endif %}
	{% if data.component_bg_value %}
	{{ data.component_bg_value }}
	{% endif %}
	} #}
	/*动态价格粗细*/
	{# .geshop-gb-component.ui-{{ pageInstanceId }} .gs-shop-price strong,.geshop-gb-component.ui-{{ pageInstanceId }} .gs-sale-price{
	{% if pageData.price_ft_weight == '1' %}
		font-weight:bold !important;
	{% else %}
		font-weight:normal !important;
	{% endif %}
	} #}


	/* 商品背景投影 */
	{# {% if pageData.base_bg_shadow == '0'%}
		.geshop-gb-component.ui-{{ pageInstanceId }} .gs-list-item,.geshop-gb-component.ui-{{ pageInstanceId }} .gs-list-item_tag {
			box-shadow: none !important;
		}
	{% else %}
		.geshop-gb-component.ui-{{ pageInstanceId }} .gs-list-item,.geshop-gb-component.ui-{{ pageInstanceId }} .gs-list-item_tag {
			box-shadow: 0px 0px 10px {{data.base_bg_shadow_color|default('#CCCCCC')}} !important;
			-webkit-box-shadow: 0px 0px 10px {{data.base_bg_shadow_color|default('#CCCCCC')}} !important;
		}
	{% endif %} #}
</style>

	{# 专题页/落地页 埋点 pagePoint #}
	{% if pageData.page_point_type|default('activityPage') ==  'activityPage' %}
		{% set pagePoint = {
			wrapLink: 'track-goodsItem',
			goodLink: 'track-goodsLink'
		}%}

	{% elseif pageData.page_point_type ==  'landPage' %}
			{% set pagePoint = {
			wrapLink: 'subjectGoodItem',
			goodLink: 'js-link'
		}%}
	{% else %}
		{% set pagePoint = {
			wrapLink: 'track-goodsItem',
			goodLink: 'track-goodsLink'
		}%}

	{% endif %}

<div class="gb_goodsWrap gb_wrapper_common geshop-component-goods" style="{% if data.goodsInfo|length == 0 and data.isEditEnv == 0 %}display:none;{% endif %}">
	<div class="gb_goods_item">
		<li  class="gb_stylenone gs-list-item {{pagePoint.wrapLink}} {{goodsStatus}}" data-track-key="{{pageData.rightGoodsInfo.goods_sn}}">
			{% if  pageData.preferential_active == "1"  %}
				{% if pageData.rightGoodsInfo == {} or pageData.rightGoodsInfo.startTime > 0  %}
					<p class="gb_times">
						<em class="icon-clock"></em><span class="gs-goodsRushDown gb_endTime" data-begin="{{pageData.rightGoodsInfo.startTime}}" data-end="{{pageData.rightGoodsInfo.endTime}}" data-stock="{{pageData.rightGoodsInfo.stock_num}}">Ends in: 00:00:00:00</span>
					</p>
				{% endif %}
			{% endif %}

			{% if  pageData.rightGoodsInfo.startTime > 0  %}
				<span class="gs-goodsRushDown" style="opacity:0;display:block;height:0;width:0;overflow:hidden;" data-begin="{{pageData.rightGoodsInfo.startTime}}" data-end="{{pageData.rightGoodsInfo.endTime}}" data-stock="{{pageData.rightGoodsInfo.stock_num}}"></span>
			{% endif %}

			<div class="gb_main">
				<a href="{{pageData.rightGoodsInfo.url_title}}" class="track-goodsLink" data-app="product" data-app-sku="{{pageData.rightGoodsInfo.web_goods_sn}}" data-app-wcode="{{pageData.rightGoodsInfo.wh_code}}"><p class="gb_goodsName">{{pageData.rightGoodsInfo.goods_title|default('This is a commodity')}}</p></a>
				<div class="gb_leftInfo">
					<div class="gb_imgBox gs-good-label-box">
						{% if pageData.recommended_active == "1" %}<div class="gb_recommended" style="{% if data.recommended_bgImage %}background:url({{data.recommended_bgImage}}) no-repeat 0 0/100% 100%;{% endif %}"><div><span>{{pageData.discount_text}}</span></div></div>{% endif %}
						{% if pageData.discount_active == "1" %}
							{% if pageData.rightGoodsInfo == {} or pageData.rightGoodsInfo.discount > 0 %}
								<div class="gb_discount gs-goods-discount--m">
									<div>
										<span class="curtoff-box">
											<strong>{{pageData.rightGoodsInfo.discount|default('50')}}%</strong>
										</span>
										<span class="curtoff-text">OFF</span>
									</div>
								</div>
							{% endif %}
						{% endif %}

						<a href="{{pageData.rightGoodsInfo.url_title}}" class="track-goodsLink" data-app="product" data-app-sku="{{pageData.rightGoodsInfo.web_goods_sn}}" data-app-wcode="{{pageData.rightGoodsInfo.wh_code}}">
							{% if  pageData.rightGoodsInfo.goods_img  %}
								<img  class="js-lazyload" data-lazy="{{pageData.rightGoodsInfo.goods_img}}"  src="{{pageData.rightGoodsInfo.goods_img}}"  title="{{pageData.rightGoodsInfo.goods_title}}" alt="{{pageData.rightGoodsInfo.goods_title}}">
							{% else %}
								<img class="js-lazyload" data-lazy="{{pageData.imgSrc}}" src="{{pageData.imgSrc}}">
							{% endif %}
							<i class="gs-goods-status" style="{% if pageData.rightGoodsInfo.startTime == 0 and pageData.rightGoodsInfo.stock_num <= 0 and pageData.rightGoodsInfo != {}  %} display:block; {% endif %}"><span>{{gb_component_trans(lang , 'sold_out')}}</span></i>
						</a>
						{# 服务标 #}
						{% set item = pageData.rightGoodsInfo %}
						{{include ('@app/files/parts/temp/gb/common/default_m/goodLabel.twig')}}
					</div>
				</div>
				<div class="gb_rightInfo">
					<div class="gb_contentBox">
						{% if pageData.rightGoodsInfo == {}  or pageData.right_contents1 != "" %} <p class="gb_contents">{{pageData.right_contents1|default('This is a product description 1')}}</p>{% endif %}
						{% if pageData.rightGoodsInfo == {}  or pageData.right_contents2 != "" %}<p class="gb_contents">{{pageData.right_contents2|default('This is a product description 2')}}</p>{% endif %}
						{% if pageData.rightGoodsInfo == {}  or pageData.right_contents3 != "" %}<p class="gb_contents">{{pageData.right_contents3|default('This is a product description 3')}}</p>{% endif %}
					</div>

					{% if pageData.rightGoodsInfo == {} or data.right_code %}
						<div class="gb_coupon_box">
							<span  class="gb_coupon"><span class="gb_right_coupon">{{data.right_code|default('ASDF58490')}}  </span> </span><span class="gb_copy" id="rightCopy"> {{gb_component_trans(lang , 'copy')}} </span>
						</div>
						<div class="gb_after_coupon">{{gb_component_trans(lang , 'after_coupon')}}</div>
					{% endif %}

					<p class="gb_price2 gs-shop-price">
						<strong class="js-currency gs-sale-price" data-currency="{{pageData.rightGoodsInfo.shop_price|default('0.00')}}">${{pageData.rightGoodsInfo.shop_price|default('0.00')}} </strong>
						{# 本店售价新增是否显示 #}
						{% if pageData.rightGoodsInfo == {} or pageData.rightGoodsInfo.discount > 0 %}
							{% if pageData.price_label_active == '1' %}
								{% if pageData.rightGoodsInfo.market_price|default('0.00') and pageData.rightGoodsInfo.shop_price|default('0.00') != pageData.rightGoodsInfo.market_price|default('0.00') and pageData.rightGoodsInfo.shop_price|default('0.00') < pageData.rightGoodsInfo.market_price|default('0.00') %}
									<span class="gb_price1">  <strike class="js-currency" data-currency="{{pageData.rightGoodsInfo.market_price}}">${{pageData.rightGoodsInfo.market_price|default('0.00')}}</strike> </span>
								{% endif %}
							{% endif %}
						{% endif %}
					</p>
					<a  class="track-goodsLink gb_buy buyLink  {% if pageData.rightGoodsInfo.stock_num <= 0 and pageData.rightGoodsInfo != {} %} cart_good_soldOut {% endif %}"  href="{{pageData.rightGoodsInfo.url_title}}" data-app="product" data-app-sku="{{pageData.rightGoodsInfo.web_goods_sn}}" data-app-wcode="{{pageData.rightGoodsInfo.wh_code}}">
						{% if pageData.rightGoodsInfo.stock_num <= 0 and pageData.rightGoodsInfo != {} %}
							{{gb_component_trans(lang , 'sold_out')}}
						{% else %}
							{{ pageData.shop_text|default(gb_component_trans(lang , 'buy_now')) }}
						{% endif %}
					</a>
					<i class="icon-cart gb_cart js-addCart {% if pageData.rightGoodsInfo.stock_num <= 0 and pageData.rightGoodsInfo != {} %}   {% if pageData.rightGoodsInfo.startTime == '0' %} cart_good_soldOut {% endif %} {% endif %}"   data-sku="{{pageData.rightGoodsInfo.goods_sku}}" data-warehousecode="{{pageData.rightGoodsInfo.wh_code}}" data-img="{{pageData.rightGoodsInfo.goods_img}}" data-qty="1" style="" ></i>
				</div>
			</div>
		</li>
	</div>
</div>
</div>




