<!-- embed stylesheet begin -->
<style type="text/css">
.geshop-U000075-fixedbottom-{{ pageInstanceId }}{
    {% if data.isEditEnv == 1 %} height: 9rem;{% endif %}
}

.geshop-U000075-fixedbottom-{{ pageInstanceId }} .main-container .drawer-trigger .center-align{
  background-color: {{data.btn_bgColor | default('#f8f8f8')}};
  {% if data.btn_bgImage %} background-image: url({{data.btn_bgImage}}); {% endif %}
} 
.geshop-U000075-fixedbottom-{{ pageInstanceId }} .main-container .drawer-trigger .center-align .deal-button{
  color: {{data.btn_fontColor | default('#000000')}};
}
.geshop-U000075-fixedbottom-{{ pageInstanceId }} .main-container .drawer-trigger .center-align:hover{
  {% if data.btn_hover_bgColor %} background: {{data.btn_hover_bgColor}}; {% endif %}
}
.geshop-U000075-fixedbottom-{{ pageInstanceId }} .main-container .deals-main-container{
  background-color: {{data.ad_bgColor | default('#f8f8f8')}};
  {% if data.ad_bgImage %} background-image: url({{data.ad_bgImage}}); {% endif %}
}
.geshop-U000075-fixedbottom-{{ pageInstanceId }} .main-container .deals-main-container .slider .swiper-button-prev{
  background-image: url({{data.left_image | default('https://geshopimg.logsss.com/uploads/s2ukJgj3Zp1X7dHz9LVyFcmfT65lb0AO.png')}}); 
}

.geshop-U000075-fixedbottom-{{ pageInstanceId }} .main-container .deals-main-container .slider .swiper-button-next{
  background-image: url({{data.right_image | default('https://geshopimg.logsss.com/uploads/zbRJa416CwhXLE8Tmt3KyNOl75QcpZrI.png')}});
}
.geshop-U000075-fixedbottom-{{ pageInstanceId }} .main-container .drawer-trigger .center-align .ico-fold{
   background-image: url({{data.dropdown_image | default('https://geshopimg.logsss.com/uploads/14XwdPGioNhlWe5zKm8bLBctp9I6fVgU.png')}});
}
.geshop-U000075-fixedbottom-{{ pageInstanceId }} .main-container .drawer-trigger .center-align .ico-fold.unfold{
   background-image: url({{data.close_image | default('https://geshopimg.logsss.com/uploads/pfYDoRQjZ6zPyCg0JNscKluEHX2hwdMF.png')}});
}
.geshop-U000075-fixedbottom-{{ pageInstanceId }} .main-container .deals-main-container .swiper-container .swiper-pagination .swiper-pagination-bullet.swiper-pagination-bullet-active{
  background-color: {{data.pagination_active_bgcolor | default('#000000')}}
}
.geshop-U000075-fixedbottom-{{ pageInstanceId }} .main-container .deals-main-container .swiper-container .swiper-pagination .swiper-pagination-bullet{
  background-color: {{data.pagination_bgcolor | default('#ededed')}}
}

</style>
<!-- embed stylesheet end -->