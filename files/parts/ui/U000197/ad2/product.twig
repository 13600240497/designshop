<div class="geshop-col-6" style="text-align: left;">
  {% if data.img1Src %}
  <a {% if data.img1Href %}href="{{ data.img1Href }}"{% endif %} class="logsss_event" style="display: block; width: {{ data.img1Width }}px; height: {{ data.img1Height }}px;"
    {% if data.isOpenNew == 1 %}target="_blank"{% endif %}
  >
    <img style="display: block; vertical-align: top;" alt="{{ data.img1Alt }}" src="{{ data.img1Src }}" data-original="{{ data.img1Src }}" width="{{ data.img1Width }}" height="{{ data.img1Height }}" class="js_gdexp_lazy js-lazyload js_gbexp_lazy"
    >
  </a>
  {% else %}
  <p style="border: 1px solid #dddddd; height: 64px; line-height: 64px; text-align: center; font-size: 16px; font-weight: bold; color: #dddddd;">
    图一(图片宽度限为590px)
  </p>
  {% endif %}
</div>
<div class="geshop-col-6" style="text-align: right;">
  {% if data.img2Src %}
  <a {% if data.img2Href %}href="{{ data.img2Href }}"{% endif %} class="logsss_event" style="display: block; width: {{ data.img2Width }}px; height: {{ data.img2Height }}px;"
    {% if data.isOpenNew == 1 %}target="_blank"{% endif %} 
  >
    <img style="display: block; vertical-align: top;" alt="{{ data.img2Alt }}" src="{{ data.img2Src }}" data-original="{{ data.img2Src }}" width="{{ data.img2Width }}" height="{{ data.img2Height }}" class="js_gdexp_lazy js-lazyload js_gbexp_lazy"
    >
  </a>
  {% else %}
  <p style="border: 1px solid #dddddd; height: 64px; line-height: 64px; text-align: center; font-size: 16px; font-weight: bold; color: #dddddd;">
    图二(图片宽度限为590px)
  </p>
  {% endif %}
</div>
