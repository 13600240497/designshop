<div class="geshop-col-4" style="text-align: left;">
  {% if data.img1Src %}
  <a {% if data.img1Href %}href="{{ data.img1Href }}"{% endif %} class="logsss_event" style="display: block; width: {{ data.img1Width }}px; height: {{ data.img1Height }}px;"
    {% if data.isOpenNew == 1 %}target="_blank"{% endif %}
  >
    <img alt="{{ data.img1Alt }}" src="{{ data.img1Src }}" data-original="{{ data.img1Src }}" style="display: block; vertical-align: top;" width="{{ data.img1Width }}" height="{{ data.img1Height }}" class="js-lazyload"
    >
  </a>
  {% else %}
  <p style="border: 1px solid #dddddd; height: 130px; line-height: 130px; text-align: center; font-size: 16px; font-weight: bold; color: #dddddd;">
    图一(图片宽度限为386px)
  </p>
  {% endif %}
</div>
