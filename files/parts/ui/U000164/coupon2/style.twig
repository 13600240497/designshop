{% set box = '#U000164_'~pageInstanceId %}

#U000164_{{ pageInstanceId }} {
    margin-top: {{ data.box_margin_top ?? 32 }}px;
    margin-bottom: {{ data.box_margin_bottom ?? 32 }}px;
}

{{ box }} .geshop-u000164-coupon2 {
    text-align : {{ data.components_align ?: 'left' }}
}

{{ box }} .u000164-coupon2-box {
    width: {{ data.bg_width ?? 389 }}px;
    height: {{ data.bg_height ?? 176 }}px;
    background: url({{ data.bg_img ?: '//geshopimg.logsss.com/uploads/zsAd9FrOlIJ8cuiRBeEfPHtMYQ3W1KaX.png' }}) 0 0 no-repeat;
    background-size: 100% 100%;
}

{{ box }} .good-range-bar {
    background-color: {{ data.count_bg ?: '#ffffff' }};
}

{{ box }} .good-range-left {
    background-color: {{ data.left_bg ?: '#222222' }};
}

{{ box }} .good-range-box {
    color: {{ data.left_color ?: '#222222' }};
}

{{ box }} .coupon2-info-group {
    width: {{ data.btn_w ?? 115 }}px;
    bottom: {{ data.btn_mb ?? 53 }}px;
    right: {{ data.btn_mr ?? 29 }}px;
}

{{ box }} .group-state-comming > li{
    background-color: {{ data.notstart_bg_color ?: '#999999' }};
    color: {{ data.notstart_text_color ?: '#FFFFFF' }};
}

{{ box }} .group-state-start > li, {{ box }} .group-state-start > li a{
    background-color: {{ data.underway_bg_color ?: '#222222' }};
    color: {{ data.underway_text_color ?: '#FFFFFF' }};
}

{{ box }} .group-state-end > li{
    background-color: {{ data.ended_bg_color ?: '#999999' }};
    color: {{ data.ended_text_color ?: '#FFFFFF' }};
}

{{ box }} .group-state-start > li.state-all-claimed{
    background-color: {{ data.ended_bg_color ?: '#999999' }};
    color: {{ data.ended_text_color ?: '#FFFFFF' }};
}

{{ box }} .getting-main {
    background-color: {{ data.model_bg_color ?: '#FFFFFF' }};
}

{{ box }} .getting-info p {
    color: {{ data.model_text_color ?: '#FFFFFF' }};
}

{{ box }} .getting-info p span{
    color: {{ data.model_title_color ?: '#FA386A' }};
}

{{ box }} .getting-info .btn-sure{
    background-color: {{ data.model_btn_bg ?: '#222222' }};
    color: {{ data.model_btn_color ?: '#FFFFFF' }};
}


{{ box }} .noEnough-main {
    background-color: {{ data.tisp_bg_color ?: '#FFFFFF' }};
}

{{ box }} .noEnough-info p , {{ box }} .noEnough-info p a{
    color: {{ data.tisp_text_color ?: '#FFFFFF' }};
}

{{ box }} .noEnough-info h3{
    color: {{ data.tisp_title_color ?: '#FA386A' }};
}

{{ box }} .noEnough-info .btn-sure{
    background-color: {{ data.tisp_btn_bg ?: '#222222' }};
    color: {{ data.tisp_btn_color ?: '#FFFFFF' }};
}

