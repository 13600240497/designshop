}

    {{ class_box }} .geshop-goods-discount{
    {% if data.m_discount_bgImage %}
        background-image: url({{ data.m_discount_bgImage }});
        border-radius:0;
    {% endif %}
        font-size: {{ data.m_discount_fontSize ?? 14 }}px;
    }

    {{ class_box }} .geshop-goods-discount p:first-child  {
        padding-top: {{ data.m_discount_pt ?? 0 }}px;
    }

    {{ class_box }} .geshop-goods-discount p{
        padding-left: {{ data.m_discount_pl ?? 0 }}px;
    }
}
