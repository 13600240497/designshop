<!-- embed stylesheet begin -->
<style type="text/css" >
    {% set class_root = '.geshop-u000230-modal1-'~pageInstanceId %}
    {% set class_box = '.geshop--u000230-modal1-'~pageInstanceId~' .geshop-container' %}
    {% set unit = 'rem' %}
    {% if data.isEditEnv== 0 %}
        .hotArea {
            background: transparent;
        }
    {% else %}
        .hotArea {
            border: 1px dotted #ffffff;
            background: rgba(30, 159, 255, .5);
        }
    {% endif %}
    {{ class_root }} {
        margin-top: {{ data.box_margin_top ?? 0 }}px;
        margin-bottom: {{ data.box_margin_bottom ?? 0 }}px;
    }

    {% include '@app/files/parts/formTemplate/components/margin-dl-style.twig' with{'space':class_root} %}
</style>
<!-- embed stylesheet end -->
