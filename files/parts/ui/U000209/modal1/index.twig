{# ips数据对象正向映射 #}
{% set ipsGoods = {} %}
{% if data.goodsSKU %}
    {% for key,item in data.goodsSKU %}
        {% set ipsGoods = ipsGoods|merge({(item.key):item}) %}
    {% endfor %}

{% endif %}

{# 调用VUE模式 #}
{%
    include '@app/files/parts/components/default/m/index_for_vue.twig' with {
        key: 'U000209',
        theme: 'modal1',
        style: true,
        preRender: true,
        preRenderType: 'goodsList',
        ipsGoods: ipsGoods
    }
%}
