
<div class="w1200 clearfix"> 
    <ul class="clearfix l-part">
        {% if data.communityRgDefaultList %}
            {% for item in data.communityRgDefaultList|slice(0,2) %}
                <li data-index="{{ loop.index }}">
                    
                    <a href="{{ item.url }}" rel="nofollow" class=""
                        {% if data.isOpenNew == 1 %}target="_blank"{% endif %}
                        data-logsss-event-value="{ 'pm':'md','p':'gs-{{ pageId }}-{{ pageInstanceId }}','ubcta':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','mdID':'{{ data.imageSrc }}','cporder':'{{ layoutIndex }}','rank':'{{ loop.index + 2 }}'} }"
                        >
                        <img class="js_gdexp_lazy js-geshopImg-lazyload js_gbexp_lazy" src="https://geshopcss.logsss.com/imagecache/geshop/resources/images/rg/loading_big.gif" data-original="{{ item.imageSrc }}" data-logsss-browser-value="{ 'pm':'md','p':'gs-{{ pageId }}-{{ pageInstanceId }}','bv':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','mdID':'{{ data.imageSrc }}','cporder':'{{ layoutIndex }}','rank':'{{ loop.index }}'} }">
                    </a>
                </li>
            {% endfor %}
        {% else %}
            <li class="buyer-show-item first js_show_item_1" data-index="1">
                <img src="{{staticDomain}}/resources/images/rg-pc/edit_icon1.png" alt="">
                
            </li>
            <li class="buyer-show-item first js_show_item_2" data-index="2">
                <img src="{{staticDomain}}/resources/images/rg-pc/edit_icon2.png" alt="">
                
            </li>
        {% endif %}
    </ul>
    <ul class="clearfix r-part">
        {% if data.communityRgDefaultList %}
            {% for item in data.communityRgDefaultList|slice(2,1) %}
                <li class="first-photo" data-index="{{ loop.index + 2 }}">
                    
                    <a href="{{ item.url }}" rel="nofollow"
                        {% if data.isOpenNew == 1 %}target="_blank"{% endif %}
                        data-logsss-event-value="{ 'pm':'md','p':'gs-{{ pageId }}-{{ pageInstanceId }}','ubcta':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','mdID':'{{ data.imageSrc }}','cporder':'{{ layoutIndex }}','rank':'{{ loop.index + 2 }}'} }"
                        >
                        <img class="js_gdexp_lazy js-geshopImg-lazyload js_gbexp_lazy" src="https://geshopcss.logsss.com/imagecache/geshop/resources/images/rg/loading_big.gif" data-original="{{ item.imageSrc }}" data-logsss-browser-value="{ 'pm':'md','p':'gs-{{ pageId }}-{{ pageInstanceId }}','bv':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','mdID':'{{ data.imageSrc }}','cporder':'{{ layoutIndex }}','rank':'{{ loop.index + 2 }}'} }">
                    </a>
                </li>
            {% endfor %}
        {% else %}
            <li class="first-photo buyer-show-item js_show_item_3" data-index="3">
                <img src="{{staticDomain}}/resources/images/rg-pc/edit_icon3.png" alt="">
            </li>
        {% endif %}

        {% if data.communityRgDefaultList %}
            {% for item in data.communityRgDefaultList|slice(3,6) %}
                <li class="" data-index="{{ loop.index + 3 }}">
                    
                    <a href="{{ item.url }}" rel="nofollow" class="btn logsss_event"
                        {% if data.isOpenNew == 1 %}target="_blank"{% endif %}
                        data-logsss-event-value="{ 'pm':'md','p':'gs-{{ pageId }}-{{ pageInstanceId }}','ubcta':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','mdID':'{{ data.imageSrc }}','cporder':'{{ layoutIndex }}','rank':'{{ loop.index + 2 }}'} }"
                        >
                        <img class="js_gdexp_lazy js-geshopImg-lazyload js_gbexp_lazy" src="https://geshopcss.logsss.com/imagecache/geshop/resources/images/rg/loading_big.gif" data-original="{{ item.imageSrc }}" data-logsss-browser-value="{ 'pm':'md','p':'gs-{{ pageId }}-{{ pageInstanceId }}','bv':{'cpID':'{{ pageInstanceId }}','cpnum':'{{ compKey }}','cplocation':'{{ uiIndex }}','mdID':'{{ data.imageSrc }}','cporder':'{{ layoutIndex }}','rank':'{{ loop.index + 3 }}'} }">
                    </a>
                </li>
            {% endfor %}
        {% else %}
            <li class="buyer-show-item  js_show_item_4" data-index="4">
                <img src="{{staticDomain}}/resources/images/rg-pc/edit_icon4.png" alt="">
                
            </li>
            <li class="buyer-show-item  js_show_item_5" data-index="5">
                <img src="{{staticDomain}}/resources/images/rg-pc/edit_icon5.png" alt="">
                
            </li>
            <li class="buyer-show-item  js_show_item_6" data-index="6">
                <img src="{{staticDomain}}/resources/images/rg-pc/edit_icon6.png" alt="">
               
            </li>
            <li class="buyer-show-item  js_show_item_7" data-index="7">
                <img src="{{staticDomain}}/resources/images/rg-pc/edit_icon7.png" alt="">
                
            </li>
            <li class="buyer-show-item  js_show_item_8" data-index="8">
                <img src="{{staticDomain}}/resources/images/rg-pc/edit_icon8.png" alt="">
            </li>
            <li class="buyer-show-item  js_show_item_9" data-index="9">
                <img src="{{staticDomain}}/resources/images/rg-pc/edit_icon9.png" alt="">
            </li>
        {% endif %}
    </ul>
</div>
